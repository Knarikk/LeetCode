def addTwoNumbers( l1,l2):
        string1 = ''
        string2 = ''
        sum = 0
        new_list = []
        for i in l1:
            i = str(i)
            string1 += i
        string1 = string1[::-1]
        for i in l2:
            i = str(i)
            string2 += i
        string2 = string2[::-1]
        sum = str(int(string1)+ int(string2))[::-1]
        for i in sum:
            new_list.append(int(i))
        print(new_list)
(addTwoNumbers([3,4,2],[4,6,5]))
